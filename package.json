{
    "name": "eleventy-react",
    "description": "Use React components in Eleventy",
    "scripts": {
        "dev": "env ELEVENTY_EXPERIMENTAL=true npx eleventy --serve",
        "build": "env ELEVENTY_EXPERIMENTAL=true npx eleventy",
        "storybook": "start-storybook -s ./_site -p 6006",
        "build-storybook": "build-storybook -o _site/storybook",
        "deploy": "npm run build && npm run build-storybook",
        "release": "release-it"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@11ty/eleventy": "^0.11.1",
        "@babel/core": "^7.12.9",
        "@babel/plugin-proposal-optional-chaining": "^7.12.7",
        "@babel/preset-react": "^7.12.7",
        "@storybook/addon-actions": "^6.1.7",
        "@storybook/addon-essentials": "^6.1.7",
        "@storybook/addon-links": "^6.1.7",
        "@storybook/react": "^6.1.7",
        "@svgr/webpack": "^5.5.0",
        "babel-loader": "^8.2.1",
        "cheerio": "^1.0.0-rc.3",
        "core-js": "^3.8.0",
        "dayjs": "^1.9.6",
        "faker": "^5.1.0",
        "fast-glob": "^3.2.4",
        "lodash": "^4.17.20",
        "prettier": "2.2.0",
        "raw-loader": "^4.0.2",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "react-is": "^16.13.1",
        "release-it": "^13.7.1",
        "styled-components": "^5.2.1",
        "webpack": "^4.44.2"
    },
    "dependencies": {
        "fisarmonica": "^1.3.5"
    },
    "version": "0.5.2"
}
